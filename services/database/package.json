{
  "name": "sourcify-database",
  "version": "2.5.4",
  "private": true,
  "description": "The Sourcify database",
  "main": "index.js",
  "scripts": {
    "migrate:up": "db-migrate up",
    "migrate:reset": "db-migrate reset",
    "migrate:create": "db-migrate create ",
    "sourcify:database": "node scripts.mjs ",
    "migrate:setup": "mkdir -p migrations-temp && ln -srf ./database-specs/migrations/* ./migrations-temp/ && ln -srf ./migrations/* ./migrations-temp/",
    "migrate:run": "npm run migrate:setup && DBMATE_MIGRATIONS_DIR=./migrations-temp dbmate migrate",
    "migrate:rollback": "npm run migrate:setup && DBMATE_MIGRATIONS_DIR=./migrations-temp dbmate rollback",
    "migrate:new": "DBMATE_MIGRATIONS_DIR=./migrations dbmate new",
    "migrate:create-db": "dbmate create",
    "migrate:drop-db": "dbmate drop",
    "migrate:load-schema": "dbmate load",
    "migrate:status": "npm run migrate:setup && DBMATE_MIGRATIONS_DIR=./migrations-temp dbmate status"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "12.1.0",
    "db-migrate": "0.11.14",
    "db-migrate-pg": "1.5.2",
    "dbmate": "^2.27.0",
    "dotenv": "16.6.1",
    "ethers": "6.15.0",
    "pg": "8.16.3",
    "readdirp": "3.6.0"
  }
}
