# Database connection settings for delete-sourcify-match.ts script
# Copy this file to .env and fill in your values

# Database type: "postgres" or "bigquery"
DATABASE_TYPE=postgres

# PostgreSQL Configuration (when DATABASE_TYPE=postgres)
SOURCIFY_POSTGRES_HOST=localhost
SOURCIFY_POSTGRES_PORT=5432
SOURCIFY_POSTGRES_DB=sourcify
SOURCIFY_POSTGRES_USER=sourcify
SOURCIFY_POSTGRES_PASSWORD=sourcify

# BigQuery Configuration (when DATABASE_TYPE=bigquery)
BIGQUERY_PROJECT_ID=your-project-id
BIGQUERY_DATASET=sourcify
# Optional: Path to service account key file (if not using default credentials)
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json
